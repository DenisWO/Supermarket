program Supermarket;

uses
  Vcl.Forms,
  Main in 'Main.pas' {Form3},
  unitMain in 'unitMain.pas' {formMain};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm3, Form3);
  Application.CreateForm(TformMain, formMain);
  Application.Run;
end.
